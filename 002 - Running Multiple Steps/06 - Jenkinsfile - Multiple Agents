pipeline {
    agent none
    stages {
    	stage('Maven') {
			agent {
				docker { 
					image 'mvn:latest' 
				}
			}
			steps {
				sh 'mvn --version'
			}
		}
		stage('NPM') {
			agent {
				docker { 
					image 'npm:latest' 
				}
			}
			steps {
				sh 'npm --version'
			}
		}
    }
}
