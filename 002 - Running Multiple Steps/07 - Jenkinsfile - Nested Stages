pipeline {
    agent none
    stages {
		stage('Sequential Stage') {
			stage('Maven') {
				agent {
					docker { 
						image 'maven:latest' 
					}
				}
				steps {
					sh 'mvn --version'
				}
			}
			stage('NPM') {
				agent {
					docker { 
						image 'node:latest' 
					}
				}
				steps {
					sh 'npm --version'
				}
			}
		}
	}
}
